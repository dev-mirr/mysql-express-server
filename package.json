{
  "name": "mysql-express-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./src/bin/www --exec babel-node --presets @babel/preset-env",
    "start2": "node ./src/bin/www",
    "test": "jest",
    "test:db": "NODE_ENV=test npm run migrate:reset && NODE_ENV=test npm run migrate && NODE_ENV=test ./node_modules/.bin/jest ./src/__tests__ --coverage",
