    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.3",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.2.5",
    "pug": "^3.0.2",
    "redis": "^3.0.2",
    "sequelize": "^6.5.1",
    "uuid4": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
